@namespace(
	uri="http://smartmatcher/familyextended2",
	prefix="r")
package data;

class Model {
	val Person[*] persons; 
}

class Person {
   attr String firstName;
   val Family[1] family;
   val Address[*] addresses;
}

class Family {
	attr String lastName;
}

class Address {
  attr String street;
  attr String zipCode;
  attr String city;
  attr String country;
}
