package com.big.tuwien.SmartMatcher.views.dom;

import javax.xml.xpath.XPathFunction;

/**
 * This interface must be implemented by all XPath extension functions which are
 * used by the SmartMatcher. It ensures that the extension functions can reference
 * the {@link DOMView} which they are based on.
 * @author alex
 *
 */
public interface XPathExtensionFunction extends XPathFunction {
	
	public void setDOMView(DOMView view);

}
