package com.big.tuwien.smartmatcher.strategy.sreuse.marshal;

import java.io.Reader;
import java.io.StringReader;
import java.io.Writer;

import com.big.tuwien.smartmatcher.strategy.sreuse.XMLFragment;

public class XMLFragmentMarshaller extends XMLMarshallerImpl {
	
	public XMLFragmentMarshaller() {
		super();
	}
	
	
	public String marshall(XMLFragment f) {
		return super.marshall(f);
	}
	
	
	public void marshall(XMLFragment f, Writer w) {
		super.marshall(f, w);
	}
	
	
	public XMLFragment unmarshall(Reader reader) {
		return (XMLFragment) super.unmarshall(reader);
	}
	
	
	public XMLFragment unmarshall(String xml) {
		return (XMLFragment) super.unmarshall(new StringReader(xml));
	}

}
