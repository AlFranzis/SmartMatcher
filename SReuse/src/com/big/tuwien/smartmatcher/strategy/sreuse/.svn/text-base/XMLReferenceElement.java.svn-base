package com.big.tuwien.smartmatcher.strategy.sreuse;

public class XMLReferenceElement extends XMLElement {
	private XMLClassElement containedIn;
	private XMLClassElement pointsTo;
	
	
	public XMLClassElement getContainedIn() {
		return containedIn;
	}
	
	
	public void setContainedIn(XMLClassElement containedIn) {
		this.containedIn = containedIn;
	}
	
	
	public XMLClassElement getPointsTo() {
		return pointsTo;
	}
	
	
	public void setPointsTo(XMLClassElement pointsTo) {
		this.pointsTo = pointsTo;
	}
	
	
	@Override
	public String toString() {
		return "XMLRefElement(" + getName() + ")";
	}

}
